{
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "eslint.alwaysShowStatus": true,
  "eslint.debug": false,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "editor.codeActionsOnSave": {
      "source.fixAll": true
  },
  "eslint.workingDirectories": [
    {
      "directory": "demo",
      "changeProcessCWD": true
    },
    {
      "directory": ".",
      "changeProcessCWD": true
    }
  ]
}
